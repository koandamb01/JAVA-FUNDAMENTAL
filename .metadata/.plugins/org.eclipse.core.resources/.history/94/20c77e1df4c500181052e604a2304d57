package object.master;

public class Ninja {
	public Ninja() {
		this.stealth = 10;
	}
	
	public void steal(Human human) {
		System.out.println("Ninja just someone health");
		this.health += human.health;
	}
	
	public void runAway() {
		System.out.println("Ninja is running away!");
		this.health -= 10;
	}
}
