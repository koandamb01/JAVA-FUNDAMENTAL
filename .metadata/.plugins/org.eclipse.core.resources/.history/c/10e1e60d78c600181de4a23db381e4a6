package calculator;

public class Calculator implements java.io.Serializable {
	private double OperandOne;
	private double OperandTwo;
	private char Operation;
	
	public Calculator(){
		this.OperandOne = 0;
		this.OperandTwo = 0;
	}
	
	public void setOperandOne(double one) {
		this.OperandOne = one;
	}
}
